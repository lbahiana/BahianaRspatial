bbox <- corner_bbox(-43.3679,-22.9357,-43.3524,-22.9276)
pech <- get_osm(bbox, source =src)
str(pech)
summary(pech)
predios <- find(pech, way(tags(k=="building")))
predios <-find_down(pech,way(predios))
predios <-subset(pech,ids=predios)
predios <- as_sp(predios,"polygons")
spplot(predios, c("timestamp"))
