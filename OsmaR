src <- osmsource_api()
bbox <- corner_bbox(-43.20363,-22.96412,-43.19995,-22.96031)
fsaud <- get_osm(bbox, source =src)
str(fsaud)
summary(fsaud)
predios <- find(fsaud, way(tags(k=="building")))
##predios <-find_down(fsaud,way(predios))
##predios <-subset(fsaud,ids=predios)
ruas <- find(fsaud, way(tags(k=="highway")))
ruas <-find_down(fsaud,way(ruas))
ruas <-subset(fsaud,ids=ruas)
ruas <-as_sp(ruas, "lines")
spplot(ruas, c("type"))
